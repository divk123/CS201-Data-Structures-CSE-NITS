//DIVASHJYOTI KAHSYAP
#include <stdio.h>

int main()
{
  int n;
  printf("Enter the n th number:\n");
  scanf("%d",&n);
  
  long long int a=0,b=1,c=0;
  
  for(int i=0;i<n-2;i++){
      
      c=a+b;
      
      a=b;
      
      b=c;
      
  }
  
  printf("The required number is %lli",c);
  
  return 0;
}
